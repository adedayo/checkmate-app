{
  "name": "checkmate-app",
  "description": "Security static analysis tool. Find secrets in source code and configuration files",
  "version": "0.0.1",
  "main": "main.js",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adedayo/checkmate.git"
  },
  "author": "Dr. Adedayo Adetoye (aka Dayo)",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "local": "./local-build-package-checkmate.sh && npm run start",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build_gauge_lib": "ng build ngx-liquid-gauge --prod && cd dist/ngx-liquid-gauge && npm link && cd ../.. && npm link ngx-liquid-gauge",
    "build": "npm run build_gauge_lib && npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "NODE_ENV=production npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "download:checkmate": "./download-checkmate.sh",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run download:checkmate && npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.8",
    "@angular/cdk": "^11.2.7",
    "@angular/common": "~11.2.8",
    "@angular/core": "~11.2.8",
    "@angular/forms": "~11.2.8",
    "@angular/material": "^11.2.7",
    "@angular/platform-browser": "~11.2.8",
    "@angular/platform-browser-dynamic": "~11.2.8",
    "@angular/router": "~11.2.8",
    "@ctrl/ngx-codemirror": "^4.1.0",
    "@fortawesome/angular-fontawesome": "^0.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@swimlane/ngx-charts": "^18.0.1",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "codemirror": "^5.60.0",
    "d3": "^6.7.0",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.13",
    "@angular-eslint/builder": "2.0.2",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/schematics": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "^11.2.7",
    "@angular/compiler": "~11.2.8",
    "@angular/compiler-cli": "^11.2.8",
    "@types/codemirror": "^0.0.109",
    "@types/jasmine": "~3.6.9",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "codelyzer": "^6.0.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "electron-reload": "1.5.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.0",
    "protractor": "~7.0.0",
    "tailwindcss": "^2.1.1",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.5",
    "wait-on": "^5.3.0",
    "webdriver-manager": "12.1.8"
  },
  "bugs": {
    "url": "https://github.com/adedayo/checkmate/issues"
  },
  "homepage": "https://github.com/adedayo/checkmate#readme"
}
